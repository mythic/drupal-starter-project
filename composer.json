{
    "name": "mythic/drupal-starter-project",
    "description": "A Drupal starter project by Mythic Digital",
    "type": "project",
    "license": "MIT",
    "homepage": "https://github.com/mythic/drupal-starter-project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-mbstring": "*",
        "composer/installers": "^1 || ^2",
        "cweagans/composer-patches": "^1.7",
        "drupal/coffee": "^1.0",
        "drupal/config_split": "^1.6",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-recommended": "^9",
        "drupal/devel": "^4.0",
        "drupal/field_group": "^3.0",
        "drupal/gin": "^3.0",
        "drupal/gin_login": "^1.0",
        "drupal/gin_lb": "^1.0",
        "drupal/gin_toolbar": "^1.0",
        "drupal/google_tag": "^1.4",
        "drupal/layout_content": "^1.0",
        "drupal/metatag": "^1.14",
        "drupal/webform": "^6.0",
        "drush/drush": "^10",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "behat/mink": "^1.10",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "dg/bypass-finals": "^1.2",
        "drupal/coder": "^8.3",
        "phpunit/phpunit": "^8.5",
        "symfony/dotenv": "^5.1",
        "symfony/phpunit-bridge": "^5.1"
    },
    "autoload-dev": {
        "files": [
            "dotenv.php"
        ]
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "extra": {
        "drupal-scaffold": {
            "file-mapping": {
                "[web-root]/.csslintrc": false,
                "[web-root]/.eslintignore": false,
                "[web-root]/.eslintrc.json": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.txt": false,
                "[web-root]/update.php": false,
                "[web-root]/web.config": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/modules/.gitignore": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/profiles/.gitignore": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/themes/.gitignore": false,
                "[web-root]/sites/example.settings.local.php": false,
                "[web-root]/sites/example.sites.php": false,
                "[web-root]/sites/development.services.yml": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/sites/default/default.services.yml": false,
                "[web-root]/sites/default/default.settings.php": false
            },
            "locations": {
                "web-root": "web/"
            },
            "overwrite": true
        },
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {}
    }
}
